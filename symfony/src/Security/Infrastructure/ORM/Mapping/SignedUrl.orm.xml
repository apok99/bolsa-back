<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\Security\Domain\Model\SignedUrl">
        <id name="token" type="string" length="64"/>
        <field name="url"/>
        <field name="userIdentifier" nullable="true"/>
        <field name="expiresAt" type="carbon_immutable" nullable="true"/>
        <field name="uses" type="integer" nullable="true"/>
    </entity>
</doctrine-mapping>
